{
  "name": "Sgrab",
  "version": "0.4.2",
  "main": "main.js",
  "description":"Screenshot grabber",
  "author":"Nick Block",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "gh-publish":"electron-builder -w -p always"
  },
  "build": {
    "win": {
      "publish": ["github", "bintray"],
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    }
  },
  "devDependencies": {
    "electron-packager": "^12.1.0",
    "electron-builder": "^20.26.0",
    "electron": "^2.0.5"
  },
  "dependencies": {
    "desktop-screenshot": "^0.1.1",
    "electron-localshortcut": "^3.1.0",
    "electron-updater": "^3.0.3",
    "imgur": "^0.2.1",
    "imgur-uploader": "^4.2.1",
    "iohook": "^0.2.0",
    "jimp": "^0.2.28",
    "keypress": "^0.2.1",
    "play": "^0.5.0",
    "play-sound": "^1.1.2",
    "sfx": "^0.1.1",
    "sound-player": "^1.0.13",
    "soundplayer": "^1.1.1",
    "update-electron-app": "^1.3.0"
  }
}
